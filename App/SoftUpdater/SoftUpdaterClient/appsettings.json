{
  "Serilog": {
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            { "Name": "Console" },
            {
              "Name": "RollingFile",
              "Args": {
                "pathFormat": "Logs\\log-{Hour}.txt",
                "fileSizeLimitBytes": 104857600,
                "retainedFileCountLimit": 24,
                "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level}] {Message}{NewLine}{Exception}",
                "buffered": true,
                "flushToDiskInterval": "00:00:10"
              }
            }
          ]
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "AllowedHosts": "*",
  "CheckUpdateSchedule": "0 * * * * *",
  "Login": "SUDebug",
  "Password": "SUDebug",
  "Architecture": "winx86-64",
  "ReleasePath": "F:\\tmp\\SUDebug\\Release",
  "DownloadedVersionField": "DownloadedVersion",
  "InstalledVersionField": "InstalledVersion",
  "CheckUpdateScheduleSelf": "0 * * * * *",
  "LoginSelf": "SUDebugSelf",
  "PasswordSelf": "SUDebugSelf",
  "ArchitectureSelf": "winx86-64",
  "ReleasePathSelf": "F:\\tmp\\SUDebug\\ReleaseSelf",
  "DownloadedSelfVersionField": "DownloadedSelfVersion",
  "InstalledSelfVersionField": "InstalledSelfVersion",
  "NextRunDateTimeField": "NextRunDateTime",
  "NextRunDateTimeSelfField": "NextRunDateTimeSelf",
  "InstallSchedule": "0 * * * * *",
  "InstallSelfSchedule": "0 * * * * *",
  "ApplicationDirectory": "F:\\SUDebug",
  "BackupDirectory": "F:\\tmp\\SUDebug\\Backup",
  "ApplicationSelfDirectory": "\\",
  "BackupSelfDirectory": "F:\\tmp\\SUDebug\\BackupSelf",
  "UpdateScript": "1: Stop\r\n2: (1) Backup\r\n3: (2) CMD \"7z x \\\"{{ReleasePath}}\\\\app.zip\\\" -o\\\"{{ReleasePath}}\\\"\"\r\n5: (2 and 3) Install\r\n6: (not 5) RollBack\r\n7: (5 or 6) Start\r\n",
  "UpdateSelfScript": "1: Stop\r\n2: (1) Backup\r\n3: (2) CMD \"7z x \\\"{{ReleasePathSelf}}\\\\app.zip\\\" -o\\\"{{ReleasePathSelf}}\\\"\"\r\n5: (2 and 3) Install\r\n6: (not 5) RollBack\r\n7: (5 or 6) Start\r\n",
  "ServiceName": "SoftUpdaterDebugService",
  "Mode": "Normal",
  "SelfUpdateTempDir": "F:\\tmp\\SUDebug\\UpdaterTemp",
  "Server": "https://localhost:5731",
  "ConnectionStrings": {
    "LocalConnection": "Data Source = F:\\dbs\\suLocalDb\\local.db"

  }
}
