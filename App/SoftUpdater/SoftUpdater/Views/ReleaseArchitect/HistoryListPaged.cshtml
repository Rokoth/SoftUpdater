@model IEnumerable<SoftUpdater.Contract.Model.ReleaseArchitectHistory>

<table class="table">
    <thead>
        <tr>
            <th>@Html.DisplayNameFor(s => s.HId)</th>
            <th>@Html.DisplayNameFor(s => s.Id)</th>
            <th>@Html.DisplayNameFor(s => s.Name)</th>
            <th>@Html.DisplayNameFor(s => s.Path)</th>
            <th>@Html.DisplayNameFor(s => s.ReleaseId)</th>           
            <th>@Html.DisplayNameFor(s => s.ChangeDate)</th>
            <th>@Html.DisplayNameFor(s => s.IsDeleted)</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>@item.HId</td>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Path</td>
            <td>@item.ReleaseId</td>           
            <td>@item.ChangeDate</td>
            <td>@item.IsDeleted</td>
        </tr>
        }
    </tbody>
</table>
